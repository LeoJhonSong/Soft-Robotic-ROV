{
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "cmake.configureArgs": [
        "-Wno-dev"
    ],
    "cSpell.words": [
        "gflags",
        "glog"
    ],
    "cmake.configureOnEdit": false,
    "files.associations": {
        "array": "cpp",
        "hash_map": "cpp",
        "hash_set": "cpp",
        "slist": "cpp",
        "initializer_list": "cpp",
        "valarray": "cpp",
        "string_view": "cpp",
        "utility": "cpp"
    },
}